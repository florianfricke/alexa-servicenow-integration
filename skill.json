{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-GB": {
          "summary": "Manage Tickets in Service Now",
          "examplePhrases": [
            "Alexa open servicenow"
          ],
          "keywords": [
            "servicenow",
            "it-service-management",
            "itsm"
          ],
          "name": "Service Now Integration",
          "description": "Connects to a ServiceNow instance and reads different number of tickets. Allows Additionally to delete and build Service Now tickets like incidents, problems and changes. ",
