        },
        "de-DE": {
          "summary": "Verwalten von Tickets im Service Now",
          "examplePhrases": [
            "Alexa öffne servicenow"
          ],
          "keywords": [
            "servicenow",
            "it-service-management",
            "itsm"
          ],
          "name": "Service Now Integration",
          "description": "Verbindet zu einer Service Now Instanz und liest eine verschieden Anzahl von Tickets aus. Erlaubt weiterhin das Erstellen und Löschen von von Tickets wie Problems, Changes und Incidents.",
          "smallIconUri": "https://github.com/florianfricke/alexa-servicenow-integration/blob/master/servicenowIcon.png"
        }
